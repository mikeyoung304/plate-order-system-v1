{
  "name": "plate-order-system",
  "version": "1.0.0",
  "description": "Voice-enabled ordering system for restaurants",
  "scripts": {
    "build:css": "npx tailwindcss -i ./app/static/css/tailwind.src.css -o ./app/static/css/tailwind.css && npx postcss ./app/static/css/style.src.css -o ./app/static/css/style.css",
    "build:css:prod": "NODE_ENV=production npx tailwindcss -i ./app/static/css/tailwind.src.css -o ./app/static/css/tailwind.css --minify && NODE_ENV=production npx postcss ./app/static/css/style.src.css -o ./app/static/css/style.css"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.17",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.6",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.17"
  },
  "private": true,
  "dependencies": {
    "puppeteer": "^24.4.0",
    "react-toastify": "^11.0.5"
  }
}
